{
  "name": "@luke-zhang-04/utils",
  "version": "3.0.0",
  "description": "Utility functions that I find myself using a lot",
  "license": "0BSD",
  "author": "Luke Zhang",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "./build.sh",
    "lint": "eslint --ext ts src --cache",
    "lint:fix": "eslint --ext ts src --cache --fix",
    "test": "NODE_ENV=test node --experimental-specifier-resolution=node --experimental-vm-modules ./__tests__/node_modules/jest/bin/jest.js --config ./jest.config.mjs",
    "typedoc": "rm -rf typedoc-output && typedoc"
  },
  "keywords": [],
  "peerDependencies": {
    "@types/node": ">= 18",
    "node-fetch": ">= 2"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    }
  },
  "dependencies": {
    "node-abort-controller": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "node-fetch": ">= 2",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-package": "^1.4.0",
    "ts-jest": "link:__tests__/node_modules/ts-jest",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.13",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./index.js"
    },
    "./node/crypto/*": {
      "require": "./cjs/node/crypto/*.js",
      "import": "./node/crypto/*.js"
    },
    "./node/crypto": {
      "require": "./cjs/node/crypto/index.js",
      "import": "./node/crypto/index.js"
    },
    "./browser/*": {
      "require": "./cjs/browser/*.js",
      "import": "./browser/*.js"
    },
    "./browser": {
      "require": "./cjs/browser/index.js",
      "import": "./browser/index.js"
    },
    "./node/*": {
      "require": "./cjs/node/*.js",
      "import": "./node/*.js"
    },
    "./node": {
      "require": "./cjs/node/index.js",
      "import": "./node/index.js"
    },
    "./*": {
      "require": "./cjs/*.js",
      "import": "./*.js"
    }
  }
}