{
  "name": "@luke-zhang-04/utils",
  "version": "1.8.0",
  "description": "Utility functions that I find myself using a lot",
  "license": "0BSD",
  "author": "Luke Zhang",
  "main": "./index.js",
  "module": "./index.js",
  "type": "module",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./index.js"
    },
    "./exportAll": {
      "require": "./cjs/exportAll.js",
      "import": "./exportAll.js"
    },
    "./exportNamed": {
      "require": "./cjs/exportNamed.js",
      "import": "./exportNamed.js"
    },
    "./exportNamedOnly": {
      "require": "./cjs/exportNamedOnly.js",
      "import": "./exportNamedOnly.js"
    },
    "./node": {
      "require": "./cjs/node/index.js",
      "import": "./node/index.js"
    },
    "./node/exportAll": {
      "require": "./cjs/node/exportAll.js",
      "import": "./node/exportAll.js"
    },
    "./node/exportNamed": {
      "require": "./cjs/node/exportNamed.js",
      "import": "./node/exportNamed.js"
    },
    "./node/exportNamedOnly": {
      "require": "./cjs/node/exportNamedOnly.js",
      "import": "./node/exportNamedOnly.js"
    },
    "./browser": {
      "require": "./cjs/browser/index.js",
      "import": "./browser/index.js"
    },
    "./browser/exportAll": {
      "require": "./cjs/browser/exportAll.js",
      "import": "./browser/exportAll.js"
    },
    "./browser/exportNamed": {
      "require": "./cjs/browser/exportNamed.js",
      "import": "./browser/exportNamed.js"
    },
    "./browser/exportNamedOnly": {
      "require": "./cjs/browser/exportNamedOnly.js",
      "import": "./browser/exportNamedOnly.js"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "./build.sh",
    "lint": "eslint --ext ts src --cache",
    "lint:fix": "eslint --ext ts src --cache --fix",
    "test": "NODE_ENV=test node --experimental-specifier-resolution=node --experimental-vm-modules ./__tests__/node_modules/jest/bin/jest.js --config ./jest.config.mjs",
    "typedoc": "rm -rf typedoc-output && typedoc"
  },
  "keywords": [],
  "peerDependencies": {
    "@types/node": ">= 12",
    "node-fetch": ">= 2"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    }
  },
  "dependencies": {
    "node-abort-controller": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.20.19",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "node-fetch": ">= 2",
    "prettier": "^2.3.2",
    "prettier-plugin-jsdoc": "^0.3.23",
    "prettier-plugin-package": "^1.3.0",
    "ts-jest": "link:__tests__/node_modules/ts-jest",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.4",
    "typedoc-github-wiki-theme": "^0.5.1",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.3.5"
  }
}
